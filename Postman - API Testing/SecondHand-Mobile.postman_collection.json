		{
			"name": "LoginV",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.variables.get(\"variable_key\");\r",
							"pm.test(\"Response status code is 200\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"pm.test(\"Email is in a valid format\", function () {\r",
							"    const responseData = pm.response.json();\r",
							"    \r",
							"    pm.expect(responseData.email).to.be.a('string').and.to.match(/^[\\w-]+(\\.[\\w-]+)*@[\\w-]+(\\.[\\w-]+)*(\\.[a-z]{2,})$/, \"Email should be in a valid format\");\r",
							"});\r",
							"\r",
							"pm.test(\"Session object is present and contains a non-empty token\", function () {\r",
							"  const responseData = pm.response.json();\r",
							"  \r",
							"  pm.expect(responseData).to.have.property('access_token');\r",
							"  pm.expect(responseData.access_token).to.exist.and.to.have.lengthOf.at.least(1, \"Token should not be empty\");\r",
							"});"
						],
